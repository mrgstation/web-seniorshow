<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Syncopate:400,700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Rubik+Mono+One&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,900&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="css/styles.css">
    <title>Drexel Graphic Design Senior Show</title>
</head>

<body>
    <section id="home-hero">
        <div id="20-logo">
            <img src="img/20-logo-sm.svg" alt="2020 Logo">
        </div>
        <!-- <img id="w-logo" src="img/westphal-logo.png" alt="Westphal logo"> -->
    </section>

    <section id="home-title">
        <div>
            <h4>GRAPHIC DESIGN</h4>
            <h1>SENIOR SHOW</h1>
        </div>
        <div id="date-block">
            <aside class="circle"></aside>
            <article>
                <h1>13</h1>
                <p>JUNE</p>
                <p>6-9 PM<p>
                <p>URBN ANNEX</p>
            </article>
        </div>
    </section>

    <section id="home-options">
        <aside>
            <div class="button-container">
                <button type="button" name="button" class="button">
                    <a href="portfolio.html">
                        <p>Preview the show</p>
                    </a>
                    <img src="img/button/button-arrow.svg" alt="">
                </button>
            </div>
            <div class="button-container">
                <button type="button" name="button" class="button">
                    <a href="meet.html">
                        <p>Meet the designers</p>
                    </a>
                    <img src="img/button/button-arrow.svg" alt="">
                </button>
            </div>
            <div> <!-- grid --> </div>
        </aside>
    </section>

    <section id="home-awards" class="section-content">
        <aside id="award-left">
            <div>
                <!-- Loop -->
            </div>
            <div>
                <!-- Timberland -->
            </div>
        </aside>

        <div id="award-right">
            <div>
                <!-- Publication -->
            </div>
            <div class="">
                <article class="">
                    <h2>AWARD-WINNING PROJECTS</h2>
                    <h3>with national recognition from</h3>
                    <p>IBM iX, Uber, American Institute of Graphic Design, and GDUSA</p>
                </article>
            </div>
        </div>
    </section>

    <section id="home-cta" class="section-content">
        <article class="button-container">
            <h1>Get show details, directions, and RSVP.</h1>
        </article>
        <div>
            <button type="button" name="button-inverted" class="button">
                <a href="about.html">
                    <p>About the show</p>
                </a>
                <img src="img/button/button-arrow.svg" alt="">
            </button>
        </div>
    </section>

    <!-- @import "footer.kit" -->

    <!-- jQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <!-- Greensock -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.0.4/gsap.min.js"></script>

    <script src="js/scripts-min.js"></script>
    <script src="js/DrawSVGPlugin.js"></script>
</body>

</html>
